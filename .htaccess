<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteBase /
	

	RewriteRule ^about([/])?$	about.php [L,QSA]
	RewriteRule ^contact([/])?$	contact.php [L,QSA]
	RewriteRule ^sendcontact([/])?$	resources/scripts/sendcontact.php [L,QSA]	
	RewriteRule ^delivery_information([/])?$ delivery_information.php [L,QSA]
	RewriteRule ^gift_vouchers([/])?$ gift_vouchers.php [L,QSA]
	RewriteRule ^privacy_policy([/])?$ privacy_policy.php [L,QSA]
	RewriteRule ^return_policy([/])?$ return_policy.php [L,QSA]
	RewriteRule ^specials([/])?$ specials.php [L,QSA]
</IfModule>